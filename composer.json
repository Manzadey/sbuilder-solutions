{
  "name": "manzadey/sbuilder-solutions",
  "type": "project",
  "keywords": [
    "sbuilder",
    "solutions"
  ],
  "require": {
    "php": "^5.4"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "classmap": ["solutions/usedesk/src"],
    "files": ["helpers/dd.php", "helpers/data_get.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "5.4"
    },
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
